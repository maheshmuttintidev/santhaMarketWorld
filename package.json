{
  "name": "santhaMarketWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf node_modules",
    "mobile:native:clean": "npx react-native clean",
    "mobile:android:clean": "cd android && ./gradlew clean && cd ../",
    "mobile:patch": "bundle exec fastlane bump",
    "mobile:minor": "npm version minor --no-git-tag-version && bundle exec fastlane bump",
    "mobile:ios:clean": "cd ./ios && xcodebuild clean && cd ../..",
    "mobile:android:ngrok:dev": "ENVFILE=.env.ngrok yarn android && yarn start",
    "mobile:android:staging:dev": "ENVFILE=.env.staging yarn android && yarn start",
    "mobile:android:production:dev": "ENVFILE=.env.production yarn android && yarn start",
    "mobile:android:build:staging:apk": "cp .env.staging .env && yarn mobile:android:clean && cd android && fastlane increment_vc && ./gradlew assembleRelease --no-daemon&& cd ../ && ./copy_apk.sh",
    "mobile:android:build:production:apk": "cp .env.production .env && yarn mobile:android:clean && cd android && fastlane increment_vc && ./gradlew assembleRelease --no-daemon&& cd ../ && ./copy_apk.sh",
    "mobile:android:build:production:aab": "cp .env.production .env && yarn mobile:android:clean && cd android && fastlane increment_vc && ./gradlew bundleRelease && cd ../ && ./copy.sh",
    "fastlane:mobile:android:staging:build": "cp .env.staging .env && cd android && fastlane staging_android_firebase_build",
    "fastlane:mobile:android:production:build": "cp .env.production .env && cd android && fastlane staging_android_firebase_build",
    "fastlane:mobile:ios:staging:testflight:build": "cp .env.staging .env && cd ./ios && fastlane ios_testflight_build",
    "fastlane:mobile:ios:production:testflight:build": "cp .env.production .env && cd ./ios && fastlane ios_testflight_build",
    "patch:version:fastlane:mobile:android:staging:build": "cp .env.staging .env && fastlane staging_android_firebase_build_without_patch_versioning",
    "patch:version:fastlane:mobile:android:production:build": "cp .env.production .env && fastlane production_android_playstore_build_without_patch_versioning",
    "patch:version:fastlane:mobile:ios:staging:testflight:build": "cp .env.staging .env && cd ./ios && fastlane ios_testflight_build_without_patch_versioning",
    "patch:version:fastlane:mobile:ios:production:testflight:build": "cp .env.production .env && cd ./ios && fastlane ios_testflight_build_without_patch_versioning",
    "ios:pod:install": "cd ios && pod install --verbose",
    "ios:pod:cache:clean": "cd ios && pod cache clean --all",
    "ios:pod:deintegrate": "cd ios && pod deintegrate",
    "ios:pod:update": "cd ios && pod install --repo-update --verbose",
    "ios:intel:pod:update": "cd ios && arch -x86_64 pod install --repo-update --verbose",
    "mobile:ios:staging:dev": "yarn ios:pod:update && ENVFILE=.env.staging yarn ios && yarn start",
    "mobile:ios:iphoneSE:staging:dev": "yarn ios:pod:update && ENVFILE=.env.staging yarn ios:iphoneSE && yarn start",
    "test:android:deep-link-url": "npx uri-scheme open dispatcher://tokens --android"
  },
